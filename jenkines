pipeline{
    agent any
    stages{
        stage("maven build"){
            steps{
            echo "maven build succesfull"
            }
        }

        stage("prod deploy"){
        when{
            Branch = main
        }
        steps{
            echo "prod deploy succesfull"
        }
        }
    
        stage("test deploy"){
        when{
            Branch = test
        }
        steps{
          echo "test deploy succesfull"  
        }
        }
  
        stage("dev deploy"){
        when{
            Branch = dev
        }
        steps{
        echo "dev deploy was succesfull"
        }
        }
    }
   
    
}
